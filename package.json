{
  "name": "my-react-template",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:ts": "tsc --noEmit",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "yarn lint:eslint -- --fix",
    "fix:prettier": "yarn lint:prettier -- --write",
    "build": "vite build",
    "test": "vitest --dom",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "happy-dom": "^7.7.0",
    "rome": "^10.0.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vitest": "^0.28.0",
    "yarn-run-all": "^3.1.1"
  }
}
